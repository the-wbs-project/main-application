{
  "name": "worker-typescript-template",
  "version": "1.0.0",
  "description": "Cloudflare worker TypeScript template",
  "main": "dist/worker.js",
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "publish": "wrangler publish --env prod",
    "dev": "miniflare ./workers-site/worker.js --kv KVDATA --kv KVAUTH --env local.env",
    "go": "npm run build && concurrently \"npm run dev\" \"npm run build:watch\"",
    "test": "jest --config jestconfig.json --verbose"
  },
  "author": "author",
  "license": "MIT OR Apache-2.0",
  "eslintConfig": {
    "root": true,
    "extends": [
      "typescript",
      "prettier"
    ],
    "rules": {
      "no-nested-ternary": "error"
    }
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.2",
    "@types/jest": "^27.0.2",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "concurrently": "^6.3.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-typescript": "^3.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.7.1",
    "service-worker-mock": "^2.0.5",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.2.2",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@cfworker/cosmos": "^1.13.4",
    "@cfworker/uuid": "^1.12.3",
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@types/cookie": "^0.4.1",
    "@types/crypto-js": "^4.0.2",
    "cookie": "^0.4.2",
    "crypto-js": "^4.1.1",
    "itty-router": "^2.6.1"
  }
}
