{
  "name": "api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "setup": "yarn",
    "build": "esbuild src/index.ts --outfile=dist/index.js --bundle --loader:.html=text --format=esm",
    "check": "tsc -noEmit --watch",
    "check2": "tsc -noEmit",
    "build-test": "concurrently \"tsc -noEmit --watch\" \"yarn run esbuild\"",
    "publish": "wrangler publish --env prod",
    "start": "wrangler dev --env dev --local true --persist --persist-to ./.kv"
  },
  "author": "author",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230321.0",
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "concurrently": "^6.3.0",
    "esbuild": "^0.17.10",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@cfworker/cosmos": "^1.13.4",
    "@cfworker/jwt": "^4.0.4",
    "hono": "^3.5.5"
  }
}
