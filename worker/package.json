{
  "name": "the-wbs-project",
  "version": "1.0.0",
  "main": "dist/worker.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "esbuild src/index.ts --outfile=dist/index.js --bundle  --loader:.html=text --format=esm",
    "publish": "wrangler publish --env prod",
    "dev": "miniflare ./dist/index.js --wrangler-env dev",
    "format": "prettier --write '**/*.{js,ts,jsx,tsx,json,yml,yaml,md,html}' --ignore-path .gitignore",
    "go": "npm run build && concurrently \"npm run dev\" \"npm run build\""
  },
  "author": "author",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^3.11.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "concurrently": "^6.3.0",
    "esbuild": "^0.15.7",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "service-worker-mock": "^2.0.5",
    "ts-jest": "^27.1.5",
    "tsup": "^5.12.9",
    "typescript": "^4.7.2",
    "wrangler": "^2.0.29"
  },
  "dependencies": {
    "@cfworker/cosmos": "^1.13.4",
    "@cfworker/uuid": "^1.12.3",
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@types/cookie": "^0.4.1",
    "@types/crypto-js": "^4.0.2",
    "cookie": "^0.4.2",
    "crypto-js": "^4.1.1",
    "itty-router": "^2.6.1"
  }
}
