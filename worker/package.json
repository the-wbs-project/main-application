{
  "name": "worker-typescript-template",
  "version": "1.0.0",
  "description": "Cloudflare worker TypeScript template",
  "main": "dist/worker.js",
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "publish": "wrangler publish --env prod",
    "dev": "miniflare ./workers-site/worker.js -p 88 --wrangler-env dev --kv KVDATA --kv KVAUTH --env local.env --kv-persist kv --watch",
    "go": "npm run build && concurrently \"npm run dev\" \"npm run build:watch\""
  },
  "author": "author",
  "license": "MIT OR Apache-2.0",
  "eslintConfig": {
    "root": true,
    "extends": [
      "typescript",
      "prettier"
    ]
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "concurrently": "^6.3.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-typescript": "^3.0.0",
    "prettier": "^2.3.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.1.3",
    "tiny-request-router": "^1.2.2"
  }
}
