{
  "name": "site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deploy": "ng build --configuration production --output-path ../worker/public",
    "serve": "ng build --configuration development --output-path ../worker/public --watch",
    "styles": "gulp watch && gulp dark && gulp transparent && gulp skin",
    "fa:setup": "npm config set @fortawesome:registry https://npm.fontawesome.com/",
    "fa:auth": "npm config set \"//npm.fontawesome.com/:_authToken\" $FONT_AWESOME_KEY",
    "kendo:auth": "yarn run kendo-ui-license activate",
    "publish": "ng build --configuration production",
    "preinstall": "npm run fa:setup && npm run fa:auth npm run kendo:auth"
  },
  "private": true,
