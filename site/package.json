{
  "name": "pm-empower",
  "version": "0.0.0",
  "scripts": {
    "setup": "npm",
    "build": "ng build",
    "purge": "rm -r .angular",
    "watch": "ng build --watch --configuration development --output-path dist",
    "deploy:test": "ng build --configuration test",
    "deploy:production": "ng build --configuration production",
    "styles:kendo": "sass src/styles/kendo.scss src/assets/css/kendo.css -I node_modules",
    "kendo:auth": "npx kendo-ui-license activate",
    "docker:globals": "npm install @angular/cli -g",
    "docker:fa": "npm config set \"@fortawesome:registry\" https://npm.fontawesome.com/ && npm config set \"//npm.fontawesome.com/:_authToken\"",
    "docker:install": "npm install --force",
    "start": "ng serve --configuration development --live-reload false --host 0.0.0.0",
    "dep": "npx madge src/main.ts --ts-config tsconfig.json --image ./deps.png",
    "stats": "ng build --stats-json && webpack-bundle-analyzer dist/site/stats.json"
  },
  "private": true,
  "dependencies": {
